{
    "compilerOptions": {
      "module": "NodeNext",
      "esModuleInterop": true,
      "target": "ESNext",
      "moduleResolution": "NodeNext",
      "sourceMap": true,
      "outDir": "dist",
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "rootDir": "."
    },
    "lib": ["ESNext"],
    "include": ["*.ts"],
    "ts-node": {"esm": true,},
  }
  