{
  "name": "soroban-oracle-project",
  "main": "expo-router/entry",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "./cron"
  ],
  "scripts": {
    "android": "expo run android",
    "check": "expo install --fix",
    "clean": "cargo clean && yarn cache clean --all && git clean -xdf",
    "doctor": "yarn dlx @yarnpkg/doctor && yarn dlx expo-doctor@latest",
    "ios": "expo run ios",
    "lint": "yarn dlx @biomejs/biome check --apply .",
    "reset": "yarn clean && npm run setup",
    "setup": "./initialize.sh ${NETWORK:-futurenet} && yarn install",
    "start": "expo start -c",
    "start:cron": "yarn workspace cron run start",
    "update": "yarn upgrade-interactive",
    "upgrade:tamagui": "yarn up '*tamagui*'@latest '@tamagui/*'@latest",
    "upgrade:tamagui:canary": "yarn up '*tamagui*'@canary '@tamagui/*'@canary",
    "vscode": "yarn dlx @yarnpkg/sdks vscode",
    "web": "expo start --web"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@react-navigation/native": "^6.1.17",
    "@soroban-react/chains": "^9.0.4",
    "@soroban-react/core": "^9.0.4",
    "@soroban-react/freighter": "^9.0.4",
    "@soroban-react/types": "^9.0.4",
    "@stellar/stellar-sdk": "^11.3.0",
    "@tabler/icons-react": "^2.47.0",
    "@tamagui/config": "latest",
    "@tamagui/constants": "latest",
    "@tamagui/font-inter": "latest",
    "@tamagui/lucide-icons": "latest",
    "@tamagui/metro-plugin": "latest",
    "@thirdweb-dev/react": "^4.4.28",
    "axios": "^1.6.8",
    "donation-contract": "file:.soroban/contracts/donation",
    "expo": "^50.0.14",
    "expo-asset": "^9.0.2",
    "expo-font": "^11.10.3",
    "expo-linking": "^6.2.2",
    "expo-router": "^3.4.8",
    "expo-status-bar": "^1.11.1",
    "jotai": "^2.7.2",
    "oracle-contract": "file:.soroban/contracts/oracle",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.51.2",
    "react-native": "0.73.6",
    "react-native-safe-area-context": "4.8.2",
    "react-native-svg": "14.1.0",
    "react-native-web": "^0.19.10",
    "react-router-dom": "^6.22.3",
    "tamagui": "latest",
    "token-contract": "file:.soroban/contracts/token"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@biomejs/biome": "^1.6.3",
    "@chakra-ui/system": "^2.6.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/abstract-provider": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/hash": "^5.7.0",
    "@ethersproject/transactions": "^5.7.0",
    "@ethersproject/web": "^5.7.1",
    "@expo/metro-runtime": "^3.1.3",
    "@safe-global/safe-core-sdk-types": "^4.0.1",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "babel-preset-expo": "^10.0.1",
    "ethers": "^5.7.2",
    "framer-motion": "^11.0.24",
    "lit-html": "^3.1.2",
    "magic-sdk": "^22.1.1",
    "metro": "^0.80.8",
    "typescript": "^5.4.3",
    "vercel": "latest"
  },
  "packageManager": "yarn@4.1.1"
}
