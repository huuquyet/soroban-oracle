{
  "buildCommand": "expo export -p web",
  "outputDirectory": "dist",
  "devCommand": "expo",
  "cleanUrls": true,
  "framework": null,
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/"
    }
  ],
  "crons": [
    {
      "path": "/cron/cron-script",
      "schedule": "0 5 * * *"
    }
  ]
}
